bindsym $mod+F5 exec --no-startup-id ~/.i3/local/thinkpad/change_display.sh

# Start i3bar to display a workspace bar (plus the system information i3status
# finds out, if available)
bar {
        status_command i3status -c ~/.i3/configs/i3status-thinkpad.conf
	# TODO: Music status, launching feedback, etc
	tray_output primary
}
